{
  "extends": "airbnb",
  "rules": {
    "indent": ["error", 4, {"SwitchCase": 1}],
    "max-len": ["error", 120],
    "no-underscore-dangle": ["off"],
    "import/no-unresolved": "off",
    "import/no-extraneous-dependencies": "off",
    "no-unused-expressions": ["error", {"allowShortCircuit": true}]
  },
  "env": {
    "node": true
  }
}